<div class="col-md-4 col-md-offset-4">
	<h1 class="text-center">新增討論版</h1>
	<%= form_for @board do |f| %>
		<%= f.label :title %>
		<%= f.text_field :title, :class => "form-control" %> <br>
		<%= f.label :description %>
		<%= f.text_area :description, :class => "form-control" %> 
		<hr>
		<%= f.submit "Create", :class => "btn btn-primary" %>
	<% end %> 
</div>